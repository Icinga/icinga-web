<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
    xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
    parent="%core.module_dir%/Cronks/config/validators.xml"
>
    <ae:configuration>

        <validators>
                <validator class="string" name="command" required="true">
                    <argument>command</argument>

                    <errors>
                        <error>Validation of command failed!</error>
                    </errors>
                </validator>

                <validator class="string" name="data" required="true">
                    <argument>data</argument>

                    <errors>
                        <error>Validation of data failed!</error>
                    </errors>
                </validator>

                <validator class="string" name="selection" required="true">
                    <argument>selection</argument>

                    <errors>
                        <error>Validation of selection failed!</error>
                    </errors>
                </validator>

                <validator class="string" name="auth" required="true">
                    <argument>auth</argument>

                    <errors>
                        <error>Validation of auth failed!</error>
                    </errors>
                </validator>
        </validators>

    </ae:configuration>
</ae:configurations>
