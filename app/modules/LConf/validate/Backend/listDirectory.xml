<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
	xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
	xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
	parent="%core.module_dir%/LConf/config/validators.xml"
>
	<ae:configuration>
		
		<validators>
			<validator name="connectionId" class="string" required="true">
				<argument>connectionId</argument>
				<errors>
					<error>Validation of connectionId failed!</error>
				</errors>
			</validator>
			<validator name="node" class="string" required="true">
				<argument>node</argument>
				<errors>
					<error>Validation of directory failed!</error>
				</errors>
			</validator>
			<validator name="filters" class="string" required="false">
				<argument>filters</argument>
				<errors>
					<error>Validation of filters failed!</error>
				</errors>
			</validator>
		</validators>
		
	</ae:configuration>
</ae:configurations>
