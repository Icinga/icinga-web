<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
    xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
    parent="%core.module_dir%/Web/config/validators.xml"
>
    <ae:configuration>

        <validators>

        <validator class="string" name="cmd" required="true">
                <arguments>
                    <argument>cmd</argument>
                </arguments>
                <errors>
                    <error>Command validation failed</error>
                </errors>
                <ae:parameters>
                    <ae:parameter name="min">1</ae:parameter>
                </ae:parameters>
        </validator>


        <validator class="string" name="data">
                <arguments>
                    <argument>data</argument>
                </arguments>
                <errors>
                    <error>Data validation failed</error>
                </errors>
                <ae:parameters>
                    <ae:parameter name="min">1</ae:parameter>
                </ae:parameters>
        </validator>
        </validators>

    </ae:configuration>
</ae:configurations>
